{
  "name": "astar-demo",
  "version": "1.0.0",
  "author": "Austin Zhu",
  "license": "MIT",
  "scripts": {
    "node": "swanky node start",
    "lint": "eslint --ext .ts,.js .",
    "deploy": "swanky contract deploy",
    "compile": "swanky contract compile -v",
